{
  "SecurityController":{
    "prefix": "security_",

    "routes": {

      "register": {
        "route": "/register",
        "methods": ["GET","POST"]
      },

      "login": {
        "route": "/login",
        "methods": ["GET","POST"]
      },

      "logout": {
        "route": "/logout"
      }

    }
  },
  "FrontController": {

    "routes": {

      "index": {
        "route": "/"
      }

    }
  },
  "FamilyController": {
    "prefix": "family_",
    "prefix_route": "/family",

    "routes":{

      "new": {
        "route": "/new",
        "methods": ["GET","POST"]
      },

      "list": {
        "route": "/list/:id"
      },

      "demand": {
        "route": "/demand/:userId/:familyId",
        "methods": "POST"
      }

    }
  },
  "UserController": {
    "prefix": "user_",
    "prefix_route": "/user",

    "routes": {

      "all": {
        "route": "/all"
      }

    }
  }
}
